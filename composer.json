{
    "name": "tiu-ram0n/flaneo-ext-brazilian-portuguese",
    "type": "flarum-extension",
    "description": "Pacote de idiomas para o português brasileiro.",
    "homepage": "https://flaneo.com.br/",	
    "keywords": ["brazilian","portuguese","language","locale","localization","translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ramon Guilherme",
            "email": "ramonguilherme@outlook.com",
            "homepage": "https://flaneo.com.br/"
        }
           
    ],
    "support": {
	    "issues": "https://github.com/tiu-ram0n/flaneo-ext-brazilian-portuguese/issues",
	    "source": "https://github.com/tiu-ram0n/flaneo-ext-brazilian-portuguese"
    },	
    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.9.0-dev"
        },	    
        "flarum-extension": {
            "title": "Flaneo ~ Brazilian Portuguese",
            "icon": {
                "image": "icon.jpg",
                "backgroundColor": "#414972",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "pt-BR",
            "title": "Português do Brasil"
        }
    }
}
